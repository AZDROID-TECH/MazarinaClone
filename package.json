{
  "name": "mazarina-clone",
  "version": "1.0.0",
  "description": "Mazarina Trade Company website clone built with Express + EJS + SQLite3",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "knex migrate:latest --knexfile db/knexfile.js",
    "seed": "knex seed:run --knexfile db/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile db/knexfile.js",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write ."
  },
  "keywords": [
    "mazarina",
    "express",
    "ejs",
    "sqlite3",
    "nodejs"
  ],
  "author": "Azdroid",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "ejs": "^3.1.9",
    "knex": "^3.0.1",
    "sqlite3": "^5.1.6",
    "bcrypt": "^5.1.1",
    "express-session": "^1.17.3",
    "i18next": "^23.7.6",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-middleware": "^3.5.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
